<!--Student-->

<h1>Edit Student Details</h1>
		<div class="details" ng-app="grievances" ng-controller="Student">
			<table>
				<tr>
					<td>Name</td>
					<td><input ng-model="name" readonly></td>
				</tr>
				<tr>
					<td>Gender</td>
					<td><input ng-model="gender" readonly></td>
				</tr>
				<tr>
			    <tr>
					<td>Department</td>
					<td><input ng-model="dep" readonly></td>
				</tr>
				<tr>
					<td>Batch</td>
					<td><input ng-model="Batch" readonly></td>
				</tr>
				<tr>
						<td>Hall Ticket</td>
						<td><input ng-model="id" readonly></td>
				</tr>
					<td>Course completion date</td>
					<td>
							<input ng-model="cdate" readonly>
					</td>
				</tr>
				<tr>
					<td>Email Id</td>
					<td><input ng-model="email"></td>
				</tr>
				<tr>
					<td>Mobile Number</td>
					<td><input ng-model="mobile" ></td>
				</tr>
				<tr>
					<td></td>
					<td><button type="button" ng-click="submit()">Update</button></td>
				</tr>
			</table>

		</div>
